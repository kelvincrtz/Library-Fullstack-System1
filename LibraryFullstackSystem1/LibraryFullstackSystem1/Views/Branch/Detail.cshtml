@model LibraryFullstackSystem1.Models.Branch.BranchDetailModel
@{
    ViewData["Title"] = "Branch Detail";
}

<div class="body-content">
    <h2>Branch Detail</h2>
    <div class="jumbotron">
        <h1>@Model.Name</h1>

        <div class="thumbnail">
            <img src="@Model.ImageUrl" class="" />
        </div>

        <p><small>@Model.Description</small></p>
        <p><small>@Model.Telephone</small></p>
        <p><small>@Model.Address</small></p>
    </div>
</div>

